{
  "name": "@alanko0511/open-meteo-js",
  "version": "0.1.3",
  "description": "A Type-safe JavaScript client for the Open-Meteo API",
  "type": "module",
  "license": "MIT",
  "author": "Alan Ko",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsdown",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "open-meteo",
    "weather",
    "forecast",
    "api",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alanko0511/open-meteo-js"
  },
  "dependencies": {
    "ky": "^1.14.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "tsdown": "^0.16.6"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
